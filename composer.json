{
    "name": "edersoares/brazilian-salary",
    "description": "Package about salaries in Brazil",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eder Soares",
            "email": "edersoares@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Brazilian\\Salary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brazilian\\Salary\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit tests --color=always",
        "phpcs": "vendor/bin/phpcs",
        "phpcbf": "vendor/bin/phpcbf"
    },
    "require": {
        "squizlabs/php_codesniffer": "^3.2"
    }
}
